<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/list_selector"
    android:divider="@null"
    android:dividerHeight="0dp"
    android:showDividers="none" xmlns:tools="http://schemas.android.com/tools">

    <nl.imarinelife.lib.utility.RobotoTextView
        android:id="@+id/text_diving_log_divennr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:padding="3dp"
        android:text="1:"
        android:textColor="#040404"
        android:textSize="15sp"
        android:textStyle="bold"
        tools:ignore="HardcodedText" />

    <nl.imarinelife.lib.utility.RobotoTextView
        android:id="@+id/text_diving_log_divelocationname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/text_diving_log_divennr"
        android:padding="3dp"
        android:text="Mjn eigen stekkie"
        android:textColor="#040404"
        android:textSize="15sp"
        android:textStyle="bold"
        tools:ignore="HardcodedText" />
    
    <nl.imarinelife.lib.utility.RobotoTextView
        android:id="@+id/text_diving_log_divingdate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/text_diving_log_divelocationname"
        android:padding="3dp"
        android:text="01-01-2013"
        android:textColor="#A4A4A4"
        android:textSize="12sp"
        android:textStyle="italic"
        tools:ignore="HardcodedText" />

    <nl.imarinelife.lib.utility.RobotoTextView
        android:id="@+id/text_diving_log_divingtime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/text_diving_log_divelocationname"
        android:layout_toRightOf="@id/text_diving_log_divingdate"
        android:padding="3dp"
        android:text="14:45"
        android:textColor="#A4A4A4"
        android:textSize="12sp"
        android:textStyle="italic"
        tools:ignore="HardcodedText" />
    
</RelativeLayout>